 
public class Greatest {
 
    public static int greatest(int number1, int number2, int number3) {
        //write some code here
        int asw = 0;
        int num = 0;
        
        if (number1 > number2){
            asw = number1;
        }
        if (number2 > number1){
            asw = number2;
        }   
        
        if (asw > number3){
            return num = asw;
        } else {
            return num = number3;
        }
        
    }
 
    public static void main(String[] args) {
        int result = greatest(2, 7, 3);
        System.out.println("Greatest: " + result);
    }
}
 
